! Normal      Bright
! --------    -------
! 0 Black      8 Black
! 1 Red        9 Red
! 2 Yellow    10 Yellow
! 3 Green     11 Green
! 4 Blue      12 Blue
! 5 Magenta   13 Magenta
! 6 Cyan      14 Cyan
! 7 White     15 White


*.foreground:   #dadae8
*.background:   #1e1e29
*.cursorColor:  #b1e3ad

! black
*.color0:       #6e6c7e
*.color8:       #6e6c7e

! red
*.color1:       #e38c8f
*.color9:       #e38c8f

! green
*.color2:       #b1e3ad
*.color10:      #b1e3ad

! yellow
*.color3:       #ebddaa
*.color11:      #ebddaa

! blue
*.color4:       #a4b9ef
*.color12:      #a4b9ef

! magenta
*.color5:       #c6aae8
*.color13:      #c6aae8

! cyan
*.color6:       #e5b4e2
*.color14:      #e5b4e2

! white
*.color7:       #dadae8
*.color15:      #dadae8

Xft.dpi:        96
Xft.rgba:       rgb
Xft.hinting:    true
Xft.autohint:   true
Xft.antialias:  true
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault

*.font:         Roboto Mono:pixelsize=22:antialias=true:autohint=true
*.shell:        /bin/sh
*.cwscale:      1.0
*.chscale:      1.0
*.borderpx:     22
*.termname:     st-256color
*.tabspaces:    8
*.minlatency:   8
*.maxlatency:   33
*.bellvolume:   0
*.blinktimeout: 800

